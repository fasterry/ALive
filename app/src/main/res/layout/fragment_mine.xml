<?xml version="1.0" encoding="utf-8"?>
<com.zcl.alive.widget.theme.ColorLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/both_match_style"
    android:background="?attr/background"
    android:orientation="vertical">

    <include layout="@layout/main_title" />


    <RelativeLayout
        android:id="@+id/rl_collect"
        style="@style/settingItemLittleMarginStyle">

        <ImageView
            style="@style/settingItemImgStyle"
            android:background="@mipmap/collection" />

        <TextView
            style="@style/settingItemTvStyle"
            android:text="@string/collection" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_settings"
        style="@style/settingItemStyle">

        <ImageView
            style="@style/settingItemImgStyle"
            android:background="@mipmap/settings" />

        <TextView
            style="@style/settingItemTvStyle"
            android:text="@string/setting" />
    </RelativeLayout>
</com.zcl.alive.widget.theme.ColorLinearLayout>
