<?xml version="1.0" encoding="utf-8"?>
<com.zcl.alive.widget.theme.ColorLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/both_match_style"
    android:background="?attr/background"
    android:orientation="vertical">

    <include layout="@layout/commen_title" />

    <WebView
        android:id="@+id/webView_news"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <com.zcl.alive.widget.LVGhost
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/circle_loading"
            style="@style/loading_style"/>
</com.zcl.alive.widget.theme.ColorLinearLayout>
